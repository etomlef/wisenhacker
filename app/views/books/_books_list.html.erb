<table>
  <tr>
    <th>Category</th>
    <th>Subcategory</th>
    <th>Title</th>
    <th>Link</th>
  </tr>
 
  <% @books.each do |book| %>
    <tr>
      <td><%= book.category %></td>
      <td><%= book.subcat%></td>
      <td><%= book.title%></td>
      <td><%= link_to book.link, book.link %></td>
    </tr>
  <% end %>
</table>